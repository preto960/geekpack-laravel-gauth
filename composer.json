{
    "name": "geekpack/laravel-gauth",
    "description": "Package to handle authentication, registration, email verification, logout, and password reset with API and Sanctum paths. Requires geekpack/laravel-api.",
    "license": "MIT",
    "keywords": [
        "geekpack",
        "laravel",
        "gauth"
    ],
    "authors": [
        {
            "name": "Juan Orellana",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.9",
        "geekpack/laravel-api": "^0.5.75",
        "inertiajs/inertia-laravel": "^1.0",
        "tightenco/ziggy": "^2.2",
        "spatie/laravel-permission": "^6.9"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Geekpack\\Gauth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Geekpack\\Gauth\\GauthServiceProvider"
            ]
        }
    },
    "version": "0.1.36"
}
